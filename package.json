{
  "name": "snatch-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Chrome extension that enables users to visually select and extract UI components from any webpage",
  "main": "src/background.js",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build && npm run copy-scripts",
    "copy-scripts": "cp src/content/selector.js dist/src/content/selector.js && cp src/background.js dist/src/background.js",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest"
  },
  "keywords": [
    "chrome-extension",
    "ui-component",
    "html-extractor",
    "css-extractor",
    "tailwind",
    "jsx",
    "react"
  ],
  "author": "Muhammad Faeez Shabbir",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.258",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^7.0.6",
    "vite-plugin-static-copy": "^3.1.1"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "css-tree": "^2.3.0",
    "html-entities": "^2.4.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
